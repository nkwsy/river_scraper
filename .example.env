# API Keys
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_MAPS_ID=your_map_id_here
CENSUS_API_KEY=your_census_api_key_here

# Configuration
RADIUS_MILES=1
OUTPUT_DIR=location_data
# osmnx config
CACHE_DIR=cache
OVERPASS_URL=http://overpass-api.de/api/
OVERPASS_RATE_LIMIT=True
OVERPASS_VERBOSE=False

# Optional Parameters
MAX_LOCATIONS=100  # Set to -1 for unlimited
SAVE_IMAGES=true
SAVE_DETAILED_JSON=true

# Census Data
CENSUS_YEAR=2019
CENSUS_DATASET=acs/acs5

# Rate Limiting
REQUESTS_PER_SECOND=10
MAX_RETRIES=3 